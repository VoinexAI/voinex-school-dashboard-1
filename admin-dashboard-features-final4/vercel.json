{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/_next/static/(.*)",
      "dest": "/_next/static/$1"
    },
    {
      "src": "^/_next/image(.*)",
      "dest": "/_next/image$1"
    },
    {
      "src": "^/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "^/(.*\\.(?:ico|json|png|jpg|jpeg|gif|svg|css|js|woff|woff2|ttf))$",
      "dest": "/$1"
    },
    {
      "src": "^/(.*)$",
      "dest": "/"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "regions": ["iad1"],
  "framework": "nextjs",
  "buildCommand": "next build",
  "installCommand": "npm install",
  "devCommand": "next dev",
  "outputDirectory": ".next"
}